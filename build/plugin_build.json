{
    "plugins": {
        "SysEventSource": {
            "path": "core",
            "name": "sysevent_source"
        },
        "SysEventService": {
            "path": "plugins/eventservice",
            "name": "sys_event_service"
        }
    },
    "rules": [
        {
            "rule": [
                {
                    "rom": "1G",
                    "ram": "1G"
                }
            ],
            "info": {
                "loadorder": {
                    "SysEventSource": {
                        "loadtime": 0
                    },
                    "SysEventService": {
                        "loadtime": 0
                    }
                },
                "pipelines": {
                    "SysEventPipeline": [
                        "SysEventService"
                    ]
                },
                "pipelinegroups": {
                    "SysEventSource": [
                        "SysEventPipeline"
                    ]
                },
                "threads": {
                    "singledthread": {
                        "SysEventSource": "sysevent_source",
                        "SysEventService": "sysevent_service"
                    }
                }
            }
        }
    ]
}